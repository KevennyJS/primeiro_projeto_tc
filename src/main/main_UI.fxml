<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="anchorID" prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextField fx:id="caminho_arquivo_importado" layoutX="73.0" layoutY="56.0" prefHeight="38.0" prefWidth="299.0" />
            <Button fx:id="btn_importar" layoutX="406.0" layoutY="63.0" mnemonicParsing="false" onAction="#onActionbtn_importar" prefHeight="25.0" prefWidth="86.0" text="Importar" />
            <Button fx:id="btn_selecionar" layoutX="406.0" layoutY="265.0" mnemonicParsing="false" onAction="#onActionbtn_selecionar" prefHeight="25.0" prefWidth="86.0" text="Selecionar" />
            <TextField fx:id="caminho_arquivo_salvo" layoutX="73.0" layoutY="258.0" prefHeight="38.0" prefWidth="299.0" />
            <Label layoutX="79.0" layoutY="24.0" prefHeight="17.0" prefWidth="139.0" text="Importe o arquivo" />
            <Label layoutX="79.0" layoutY="221.0" prefHeight="17.0" prefWidth="232.0" text="Onde deseja salvar o arquivo?" />
            <Label layoutX="217.0" layoutY="119.0" prefHeight="25.0" prefWidth="167.0" text="Qual operação deseja realizar?" />
            <Button fx:id="btn_salvar" layoutX="228.0" layoutY="320.0" mnemonicParsing="false" onAction="#onActionbtn_salvar" prefHeight="56.0" prefWidth="167.0" text="Salvar" />
